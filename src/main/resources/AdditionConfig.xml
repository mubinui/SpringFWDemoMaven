<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean name="a1" class="org.example.ConsInj.Addition">
        <constructor-arg value="10" type="int"></constructor-arg>
        <constructor-arg value="20"></constructor-arg>
    </bean>
    <bean name="a2" class="org.example.ConsInj.Addition">
        <constructor-arg value="50" type="double"></constructor-arg>
        <constructor-arg value="60" type="double"></constructor-arg>
    </bean>
    <bean name="a3" class="org.example.ConsInj.Addition">
        <constructor-arg value="100" index="0"></constructor-arg>
        <constructor-arg value="500" index="1"></constructor-arg>
    </bean>
<!--    ambiguity solver-->
<!--    index order or type -->

</beans>